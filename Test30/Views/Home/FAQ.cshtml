@{
    if (HttpContext.Current.Session["usedID"] != null)
    { Layout = null; }
}


<!DOCTYPE html>
<html lang="en">
<head>
    <title>Count Your Eggs: FAQs</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat|Open+Sans&display=swap" rel="stylesheet">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
    @Styles.Render("~/Content2/count-your-eggs-stylesheet.css")
</head>

<body>
    <div class="hero-image">
        <div class="hero-text">
            <picture>
                <source media="(max-width: 767px)" srcset="https://github.com/Desiarmstrong/Capstone-Webpages/blob/master/cye-title-img-mobile.png?raw=true">
                <img src="https://github.com/Desiarmstrong/Capstone-Webpages/blob/master/cye-title-img.png?raw=true" id="titlelogo" width="60%" alt="Count Your Eggs Inventory Management">
            </picture>
        </div>
    </div>
    <nav class="navbar">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Index", "Home")"><img src="https://github.com/Desiarmstrong/Capstone-Webpages/blob/master/cye-initials.png?raw=true" id="navlogo" alt="Count Your Eggs" width="115px"></a>
            </div>
            <div class="collapse navbar-collapse" id="myNavbar">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Pricing", "ProductsPage", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Connect", "Contact", "Home")</li>
                </ul>
                <ul class="nav navbar-nav navbar-right">


                    @{ if (HttpContext.Current.Session["usedID"] == null)

                        {
                            <li>
                                <a href=@Url.Action("Index", "Login")><span class="glyphicon glyphicon-user"></span> Login/Register</a>
                            </li>
                        }
                    }

                    @{ if (HttpContext.Current.Session["usedID"] != null)
                        {
                            <li>
                                <a href=@Url.Action("Index", "Dashboard")><span class="glyphicon glyphicon-list-alt"></span> Inventory</a>
                            </li>
                            <li>
                                <a href="@Url.Action("LogOut","Login")"><span class="glyphicon glyphicon-user"></span> Logout</a>
                            </li>
                        }
                    }




                </ul>
            </div>
        </div>
    </nav>
<div class="container-fluid" style="background: linear-gradient( #ffffff, rgba(255,117,34,0.60))">
    <div class="box">
        <p class="heading">FAQs</p>
        <div class="faqs">
            <details>
                <summary>Covid-19 Updates</summary>
                <p class="text">We are keeping track of our orders and items that we are sending to our customers. But if you want to get an update about Covid-19, then go to this website, <a href="https://www.worldometers.info/coronavirus/?utm_campaign=homeAdvegas1?%20" target="_blank">Covid-19 World Update</a></p>
            </details>
            <details>
                <summary>How do I get started with Count-Your-Eggs-Inventory-Management?</summary>
                <p class="text">You will have to create your account in order to track your inventory.</p>
            </details>
            <details>
                <summary>Will I have to pay for the services?</summary>
                <p class="text">No, our services are free.</p>
            </details>
            <details>
                <summary>Do I have to create an account?</summary>
                <p class="text">Yes, you must create an account in order use our services. You will have create a uername and password to track your inventory.</p>
            </details>
            <details>
                <summary>Will my information be safe?</summary>
                <p class="text">Yes, we take security highly very seriously!</p>
            </details>
            <details>
                <summary>Will I be able to keep track of my invoices, invoice items, items, payments, and reciepts?</summary>
                <p class="text">Yes, you will be able to keep track of all of your inventory items.</p>
            </details>
        </div>
    </div>
</div>
<footer>
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-12">
                <a href="https://www.facebook.com/Count-Your-Eggs-Inventory-Management-107284634211664/" target="_blank" class="fa fa-facebook"></a>
                <a href="https://twitter.com/counteggs" target="_blank" class="fa fa-twitter"></a>
                <a href="@Url.Action("ComingSoon", "Home")" target="_blank" class="fa fa-youtube"></a>
                <a href="@Url.Action("ComingSoon", "Home")" target="_blank" class="fa fa-instagram"></a>


            </div>
        </div>


    </div>
    <br>
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-2 col-md-offset-3">

                <ul>
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Pricing", "ProductsPage", "Home")</li>
                    <li><a href="@Url.Action("ComingSoon", "Home")">Terms of Service</a></li>
                    <li><a href="@Url.Action("ComingSoon", "Home")">Privacy Policy</a></li>
                    <li><a href="@Url.Action("ComingSoon", "Home")">Sitemap</a></li>
                </ul>
            </div>
            <div class="col-md-2">
                <ul>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li><a href="@Url.Action("FAQ", "Home")">FAQ</a></li>
                    <li><a href="@Url.Action("ComingSoon", "Home")">Blog</a></li>
                    <li><a href="@Url.Action("ComingSoon", "Home")">Careers</a></li>
                </ul>
            </div>
            <div class="col-md-2">
                <ul>
                    <li>@Html.ActionLink("Connect", "Contact", "Home")</li>
                    <li><a href="tel:1-555-555-5555">Sales: (555) 555-55555</a></li>
                    <li><a href="tel:1-555-555-5555">Support: (555) 555-5555</a></li>
                    <li>555 Parham Rd.<br />Beverly Hills, CA 90210</li>
                </ul>
            </div>
        </div>
    </div>
</footer>
</body>
</html>